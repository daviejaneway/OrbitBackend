api Main
    type Main(argc Int32, argv [[Int8]])

    trait TraitA(x Int)
    trait TraitB()

    type TypeA(x Int) : TraitA
    type TypeB() : TraitB
    type TypeC(x Int) : TraitA

    (self TraitA) foo () (Int)
        debug self.x
        return self.x
    ...

    (self Main) main () (Int32)
        a = TypeA(1)
        b = TypeB()
        c = TypeC(3)

        a.foo()
        c.foo()

        return self.argc
    ...
