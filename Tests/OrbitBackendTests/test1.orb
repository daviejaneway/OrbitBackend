api Main {
    trait Debuggable() {
        (self Self) stringValue () (String)
    }

    type Main(argc Int32, argv [[Int8]])

    type Foo(x Int) : Debuggable

    (self Foo) stringValue () (String) {
        return "FOO"
    }

    (self Main) main () (Int32) {
        f = Foo(123)

        debug f.stringValue()

        return self.argc
    }
}
